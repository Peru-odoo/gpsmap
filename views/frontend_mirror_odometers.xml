<odoo>
    <template id='frontend_mirror_odometers'>
        <t>
            <div id="odometer" >
                <div id="distance" style="z-index:112;">000000000</div>
                <div id="date" style="z-index:112;">0000-00-00</div>
                <div id="time" style="z-index:112;">00:00</div>

                <div id="speed" >
                    <t t-call="gpsmap.frontend_odometer_speed"/>         
                </div>
                <div id="batery" >
                    <t t-call="gpsmap.frontend_odometer_batery"/>                     
                </div>

                <div id="gas" >
                    <t t-call="gpsmap.frontend_odometer_gas"/> 
                </div>                               
            </div>                

        </t>
    </template>
</odoo>
